        "    # self.cross_attentions = [CrossAttentionLayer(self.embed_dim, self.embed_dim) for i, cs in enumerate(self.chunk_starts)]\n",
        "\n",
        "  def call(self, inputs, training):\n",
        "    x = inputs\n",
